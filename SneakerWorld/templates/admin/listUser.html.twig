{% extends 'base.html.twig' %}

{% block body %}
    {% include './/common/headeradmin.html.twig' %}


    <main role="main" class="container">
        <div class="row">
            <table class="table">
                <thead>
                <tr>
                    <th scope="col">Email</th>
                    <th scope="col">{{ 'common.prenom'|trans }}</th>
                    <th scope="col">{{ 'common.nom'|trans }}</th>
                </tr>
                </thead>
                <tbody>
                {% for user in users %}
                <tr>
                    <th>{{ user.email }}</th>
                    <td>{{ user.prenom }}</td>
                    <td>{{ user.nom }}</td>

                    {% endfor %}
                </tbody>
            </table>
        </div>
    </main>


{% endblock %}
